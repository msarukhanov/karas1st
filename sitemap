// ID : Title ---> URL --> [Parent]
homepage                 : @(Homepage)                 --> /

homepage_en                 : @(Homepage)                 --> /en/
contact_en                  : @(Contact)                  --> /en/contact/                    --> homepage_en
products_en                 : @(All items)                --> /en/shop/                       --> homepage_en
new_en                      : @(New items in the store)   --> /en/shop/new/                   --> products_en
top_en                      : @(Top items)                --> /en/shop/top/                   --> products_en
popular_en                  : @(Popular items)            --> /en/shop/popular/               --> products_en
checkout_en                 : @(Checkout)                 --> /en/checkout/                   --> homepage_en
order_en                    : @(Order)                    --> /en/checkout/{0}/               --> checkout_en
payment_en                  : @(Checkout Payment)         --> /en/checkout/{0}/{1}/           --> checkout_en
account_en                  : @(Account)                  --> /en/account/                    --> homepage_en
settings_en                 : @(Account Settings)         --> /en/account/settings/           --> account_en
category_en                 : @(Category)                 --> /en/shop/*                      --> products_en
detail_en                   : @(Product)                  --> /en/product/{0}/                --> category_en
blogs_en                    : @(Blogs)                    --> /en/blogs/                      --> homepage_en
blogsdetail_en              : @(Blog detail)              --> /en/blogs/{0}/                  --> blogs_en

